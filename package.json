    "@types/lodash": "^4.14.168",
    "apollo-server-express": "^2.24.1",
    "bcrypt": "^5.0.1",
    "concurrently": "^6.0.2",
    "cors": "^2.8.5",
    "cypress": "^7.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.5",
